    "lucide-react": "0.555.0",
    "markdown-to-jsx": "9.2.0",
    "next": "16.0.7",
    "next-themes": "0.4.6",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "swiper": "12.0.3",
